{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "DISABLE_TELEMETRY": "1",
    "CLICOLOR": "1",
    "CLICOLOR_FORCE": "1",
    "CLAUDE_BASH_MAINTAIN_PROJECT_WORKING_DIR": "1",
    "GH_PAGER": "cat"
  },
  "permissions": {
    "allow": [
      "mcp__*",
      "Bash(gsed:*)",
      "Bash(git:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(gh:*)",
      "Bash(grep:*)",
      "Bash(cargo:*)",
      "Bash(RUST_LOG=debug cargo:*)",
      "Bash(kill:*)",
      "Bash(curl:*)",
      "Bash(wget:*)",
      "Bash(cp:*)",
      "Bash(rustc:*)",
      "Bash(timeout:*)",
      "Bash(rg:*)",
      "Bash(sed:*)",
      "Bash(mkdir:*)",
      "Bash(chmod:*)",
      "Bash(cargo build:*)",
      "Bash(mv:*)",
      "Bash(ls:*)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "WebFetch(domain:pypi.org)"
    ],
    "deny": []
  },
  "model": "sonnet[1m]",
  "hooks": {},
  "enableAllProjectMcpServers": true
}
